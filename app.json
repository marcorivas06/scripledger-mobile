{
  "expo": {
    "name": "scripLedger",
    "slug": "scripLedger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcosrivas1806.scripLedger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marcosrivas1806.scripLedger"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-Black.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-Bold.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-ExtraBold.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-Light.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-Medium.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-Regular.ttf",
            "./assets/fonts/DarkerGrotesque/static/DarkerGrotesque-SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
